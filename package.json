{
  "name": "amora",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "generate:t": "yarn plop --plopfile generators/templatePlop.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@styled-icons/boxicons-solid": "^10.46.0",
    "@styled-icons/material-outlined": "^10.46.0",
    "@types/lodash.xor": "^4.5.6",
    "@types/react-dom": "^17.0.2",
    "graphql": "^15.5.1",
    "joi": "^17.6.0",
    "next": "^12.3.0",
    "next-auth": "^4.10.3",
    "next-pwa": "^5.3.1",
    "polished": "^4.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-slick": "^0.29.0",
    "styled-components": "^5.3.5",
    "styled-icons": "^10.46.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@storybook/addon-essentials": "6.4.17",
    "@storybook/builder-webpack5": "^6.4.17",
    "@storybook/manager-webpack5": "^6.4.17",
    "@storybook/react": "6.4.17",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^27.4.0",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^17.0.14",
    "@types/react": "18.0.9",
    "@types/react-slick": "^0.23.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "apollo": "^2.33.4",
    "cypress": "^10.8.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.4.7",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^12.3.3",
    "lodash.xor": "^4.5.0",
    "msw": "^0.47.3",
    "plop": "^3.0.5",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-next-router": "^3.0.8",
    "typescript": "^4.5.5",
    "webpack": "5.68.0"
  },
  "resolutions": {
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.2"
  }
}
